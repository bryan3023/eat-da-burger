
<div class="col-12 col-md-6">
  <h3>Burgers to Eat</h3>
    {{#each uneaten}}
      <div class="form-group form-inline">
    	  <label class="col col-form-label text-left">{{name}}</label>
        <button
          class="btn btn-primary devour text-right"
          value="{{id}}"
          type="submit">Devour It</button>
      </div>
    {{/each}}
  </ul>
</div>

<div class="col-12 col-md-6">
  <h3>Burgers I've Eaten</h3>
    {{#each eaten}}
      <div class="form-group form-inline">
    	  <label class="col col-form-label">{{name}}</label>
      </div>
    {{/each}}
  </ul>
</div>
